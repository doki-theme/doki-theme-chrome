{
	"name": "Doki Theme: Firefox Edition",
	"version": "1.0.0",
	"description": "A theme collection of girls from various anime, manga, and visual novels series.",
	"manifest_version": 2,
	"icons": {
		"48": "icons/doki-theme-logo@48.png",
		"96": "icons/doki-theme-logo@96.png"
	},
	"permissions": [
		"theme",
		"tabs",
		"activeTab",
		"search"
	],
	"background": {
		"scripts": [
			"resources.js",
			"background.js"
		]
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/doki-theme-logo@16.png",
			"32": "icons/doki-theme-logo@32.png",
			"64": "icons/doki-theme-logo@64.png"
		},
		"default_popup": "popup/index.html",
		"default_title": "Doki Theme"
	},
	"browser_specific_settings": {
		"gecko": {
			"strict_min_version": "63"
		}
	}
}