{
	"name": "Doki Theme: Firefox Edition",
	"version": "0.0.1",
	"description": "A dark theme modeled after Ishtar from Fate",
	"manifest_version": 2,
	"icons": {
		"48": "icons/doki-theme-logo@48.png",
		"96": "icons/doki-theme-logo@96.png"
	},
	"permissions": [
		"theme",
		"tabs",
		"activeTab"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/doki-theme-logo@16.png",
			"32": "icons/doki-theme-logo@32.png",
			"64": "icons/doki-theme-logo@64.png"
		},
		"default_popup": "popup/index.html",
		"default_title": "Doki Theme"
	},
	"web_accessible_resources":[
		"waifus/ishtar/dark/ishtar-dark.png"
	]
}